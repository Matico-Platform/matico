    "deploy": "gh-pages -d editor/build"
  },
  "devDependencies": {
    "concurrently": "^6.3.0",
    "gh-pages": "^3.2.3"
  },
  "packageManager": "yarn@3.0.2",
  "dependencies": {
    "@parcel/transformer-react-refresh-wrap": "^2.0.0"
  }
