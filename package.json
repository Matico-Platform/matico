{
  "name": "matico",
  "version": "1.0.0",
  "description": "Matico monorepo of parts",
  "main": "index.js",
  "repository": "git@github.com:stuartlynn/matico_spec_test.git",
  "author": "Stuart Lynn <stuart.lynn@gmail.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "./maticojs",
    "./matico_spec/pkg/",
    "./matico_components",
    "./editor"
  ],
  "homepage": "https://stuartlynn.github.io/matico_spec_test",
  "scripts": {
    "predeploy": "yarn run build",
    "start": "concurrently \"cd matico_components && yarn watch\" \"cd editor && yarn start\" ",
